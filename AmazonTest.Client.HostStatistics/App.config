  <unity xmlns="http://schemas.microsoft.com/practices/2010/unity">
    <alias alias="ILogger" type="MyApp.ILogger, MyApp" />
    <namespace name="MyApp.Implementations" />
    <assembly name="MyApp" />

    <container>
      <register type="ILogger" name="special" mapTo="SpecialLogger" />
    </container>

  </unity>
  <system.serviceModel>
    <bindings />
    <client />
  </system.serviceModel>
